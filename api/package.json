    "express": "^4.17.1",
    "express-validator": "^6.0.1",
    "nodemailer": "^6.2.1",
    "passport": "^0.4.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "^0.0.36",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/nodemailer": "^6.2.0",
    "@types/passport": "^1.0.0",
